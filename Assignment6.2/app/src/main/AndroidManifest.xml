<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jasonmckay.assignment62">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".services.ManufacturingService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.ClearManufacturingService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.VehicleSearchService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.InsertVehicleService"
            android:exported="false" />
        <service
            android:name=".services.RemoveVehicleService"
            android:exported="false" />
        <service
            android:name=".services.UpdateVehicleService"
            android:exported="false" />
        <service
            android:name=".services.TotalCostCarService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.TotalCostBikeService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.ListVehicleService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.VehicleBuildService"
            android:exported="false"></service>
    </application>

</manifest>